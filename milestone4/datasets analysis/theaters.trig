    fasdat:Compañia_Ana_Morales a fasont:Event ;
        rdfs:label "Compañia Ana Morales"^^xsd:string ;
        fasont:calendar_summary "Mi 20 April 2016 21:00 Uhr."@de,
            "We 20 April 2016 21:00 hour."@en,
            "Mi 20 Abril 2016 21:00 Hora."@es,
