    fasdat:Geen_paniek a fasont:Event ;
        rdfs:label "Geen paniek"^^xsd:string ;
        fasont:calendar_summary "Di 12 April 2016 20:30 Uhr."@de,
            "Tu 12 April 2016 20:30 hour."@en,
            "Ma 12 Abril 2016 20:30 Hora."@es,
            "Ma 12 Avril 2016 20:30 heure."@fr,
            "Ma 12 aprile 2016 20:30 Ora."@it,
            "Di 12 april 2016 20:30 uur."@nl ;
        fasont:long_description """<h2>Met Marjolijn van Kooten en Bram Bakker</h2>
<p>Beiden zijn bekend met paniek. Marjolijn schiet zelf regelmatig in de vlekken en schreef hier het boek Schijtluis over. Zij vertelt met zelfspot en zingt ontroerende liedjes over haar leven als bangerik.</p>
<p>&nbsp;</p>
<p>Bram behandelt bekende en onbekende Nederlanders met angstklachten. Nu waagt de bekendste psychiater van Nederland zich voor het eerst in het theater. Naast alle treurigheid valt er in de psychiatrie gelukkig ook veel te lachen.</p>"""@nl ;
        fasont:main_media "https://media.iamsterdam.com/ndtrc/Images/21/2186f90c-6d47-43ca-9b52-db5bbbb51a3b/f1363aed-e5bb-4d64-ac1c-2e7dc8029777.jpg"^^xsd:anyURI ;
        fasont:playVenue fasdat:Griffioen ;
        fasont:short_description "Twee voorstellingen op één avond met als doel het taboe op angst te doorbreken. Te zien in Theater Griffioen in Amstelveen op 12 april 2016."@nl ;
        fasont:single_date "2016-04-12"^^xsd:date ;
        fasont:url "http://www.griffioen.vu.nl/"^^xsd:anyURI .

