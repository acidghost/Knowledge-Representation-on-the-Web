    fasdat:Pumps_en_Penalties a fasont:Event ;
        rdfs:label "Pumps en Penalties"^^xsd:string ;
        fasont:calendar_summary """Di 22 März 2016 20:15 Uhr,
Do 21 April 2016 20:15 Uhr."""@de,
            """Tu 22 March 2016 20:15 hour,
Th 21 April 2016 20:15 hour."""@en,
            """Ma 22 Marzo 2016 20:15 Hora,
