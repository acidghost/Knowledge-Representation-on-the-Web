        rdfs:label "Echt Gebeurd"^^xsd:string ;
        fasont:calendar_summary "Mehrere Daten: ab So 20 September 2015 bis So 15 Mai 2016."@de,
            "Multiple data: from Su 20 September 2015 to Su 15 May 2016."@en,
            "Múltiples fechas: desde Do 20 Septiembre 2015 Hasta Do 15 Mayo 2016."@es,
