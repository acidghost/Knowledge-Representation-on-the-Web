            "https://media.iamsterdam.com/ndtrc/Images/20101124/22a9bc62-984a-4e1f-bfd7-d82b941b3c52.jpg"^^xsd:anyURI,
            "https://media.iamsterdam.com/ndtrc/Images/20101124/428a089d-f093-4bd0-9bc8-3deac719333c.jpg"^^xsd:anyURI,
            "https://media.iamsterdam.com/ndtrc/Images/20101124/fcae61cc-1094-4ab8-b0f4-b49446c7efd4.jpg"^^xsd:anyURI,
            "https://media.iamsterdam.com/ndtrc/Images/20101124/fd0ebaac-74ec-4d05-a79f-cdedaa4e4abc.jpg"^^xsd:anyURI,
